{
  "cacheDirectory": "node_modules/.cache",
  "testEnvironment": "jsdom",
  "collectCoverageFrom": [
    "src/**/*.js"
  ],
  "coveragePathIgnorePatterns": [
    "src/index.js",
    "src/cli.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}
